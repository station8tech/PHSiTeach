<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
    id        = "stat8.PHSiTeach"
    version   = "2.1">
<gap:plugin name="org.apache.cordova.splashscreen" version="0.3.4" />
<gap:plugin name="com.phonegap.plugin.statusbar" version="1.1.0" />
<gap:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
<gap:plugin name="nl.x-services.plugins.backgroundaudio" version="1.0.0" />
<gap:platform name="ios" />
<preference name="webviewbounce" value="false" />

<preference name="deployment-target" value="7.0" />
<preference name="target-device" value="tablet" />



<name>PHSiTeach</name>

<access origin="*" />
<description>
PHS iTeach is an app that allows teachers and administrators of Providence Junior-Senior High School to share their educational content with their students. Lesson plans, presentations, videos, and announcements are all examples of content that can be shared with students.
</description>

<author href="https://www.stat8.net" email="support@stat8.net">
Station Eight Technologies
</author>

